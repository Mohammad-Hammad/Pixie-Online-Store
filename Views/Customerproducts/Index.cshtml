@model IEnumerable<WebApplication11.Models.Customerproduct>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Quantity)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Datefrom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dateto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Customer)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Product)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Quantity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Datefrom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dateto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Customer.CusId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Product.ProId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CpId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CpId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CpId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
