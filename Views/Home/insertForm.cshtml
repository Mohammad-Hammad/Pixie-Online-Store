
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>insertForm</title>
</head>
<body>
    <form asp-controller="Home" asp-action="insertForm" enctype="multipart/form-data">
        <label>fullname</label>
        <input type="text" placeholder="Enter Full Name" id="fullname" name="fullname"/>
        <label>FirstName</label>
        <input type="text" placeholder="Enter First Name" id="firstname" name="firstname"/>
        <label>LastName</label>
        <input type="text" placeholder="Enter Last Name" id="lastname" name="lastname"/>
        <label>UserName</label>
        <input type="text" placeholder="Enter User Name" id="username" name="username"/>
        <label>Password</label>
        <input type="password" placeholder="Enter Password" id="password" name="password"/>
        <button type="submit">SignIn</button>
    </form>
</body>
</html>
